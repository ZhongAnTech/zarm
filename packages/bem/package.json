{
  "name": "@zarm-design/bem",
  "version": "0.0.1",
  "description": "bem struct",
  "keywords": [
    "bem",
    "sass"
  ],
  "author": "JeromeLin <jerome.lin@zhongan.com>",
  "homepage": "https://github.com/ZhongAnTech/zarm#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./sass": "./lib/sass/index.scss"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZhongAnTech/zarm.git"
  },
  "scripts": {
    "clean": "rimraf types lib es dist coverage",
    "build:types": "rimraf types && tsc --outDir types -d --emitDeclarationOnly",
    "build:es": "rimraf es && zarm build --mode es --path src --out-dir es --copy-files",
    "build:lib": "rimraf lib && zarm build --mode lib --path src --out-dir lib --copy-files",
    "build": "yarn build:types && yarn build:lib && yarn build:es",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@zarm-design/cli": "^0.0.40"
  },
  "bugs": {
    "url": "https://github.com/ZhongAnTech/zarm/issues"
  }
}
