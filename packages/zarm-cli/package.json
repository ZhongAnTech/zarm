{
  "name": "@zarm-design/cli",
  "version": "0.0.39",
  "description": "Cli Tools for Zarm Design Project",
  "keywords": [
    "zarm-cli",
    "cli",
    "react",
    "tools"
  ],
  "license": "MIT",
  "author": "jerome.lin@zhongan.com",
  "typings": "types/index.d.ts",
  "bin": {
    "zarm": "./bin/zarm.js"
  },
  "files": [
    "lib",
    "bin",
    "types"
  ],
  "scripts": {
    "clean": "rimraf types lib coverage",
    "test": "jest --config ./jest.config.js",
    "coverage": "rimraf coverage && yarn test --coverage",
    "build:types": "rimraf types && tsc --outFile types/index.d.ts -d --emitDeclarationOnly",
    "build:lib": "rimraf lib && babel src --extensions .ts --out-dir lib --copy-files",
    "build": "yarn build:types && yarn build:lib",
    "dev": "npm run build -- -w"
  },
