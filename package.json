{
  "name": "dragon-mobile-ui",
  "version": "0.0.34",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run deploy && npm run dev",
    "clean": "rm -rf lib && rm -rf dist",
    "dev": "node devServer.js",
    "pro": "rm -rf assets && webpack --config webpack.config.pro.js",
    "deploy": "rm -rf node_modules && npm install && npm run build",
    "lint": "./node_modules/.bin/eslint components --ext '.js,.jsx'",
    "build:lib": "babel components --out-dir lib",
    "build:umd": "webpack --config webpack.config.umd.js",
    "build:umd:min": "webpack -p --config webpack.config.umd.min.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min"
  },
  "dependencies": {
    "classnames": "^2.1.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.21",
    "babel-loader": "^5.3.2",
    "bundle-loader": "^0.5.0",
    "css-loader": "^0.15.6",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "moment": "^2.18.0",
    "node-sass": "^3.4.2",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.5.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.0",
    "zscroller": "^0.2.5"
  },
  "tags": [
    "react",
    "dragon"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "dragon-ui",
    "dragon-mobile-ui",
    "dragon",
    "mobile"
  ],
  "description": "UI for react.js",
  "author": "Jerome.Lin <16908091@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/JeromeLin/dragon-ui"
  },
  "bugs": {
    "url": "https://github.com/JeromeLin/dragon-ui/issues"
  }
}
