{
  "name": "site-next",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "dumi build",
    "dev": "dumi dev"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "ahooks": "^3.7.6",
    "dayjs": "^1.11.3",
    "lodash": "^4.17.15",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.4.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "babel-plugin-import": "^1.13.6",
    "classnames": "^2.3.2",
    "dumi": "^2.2.1"
  }
}
