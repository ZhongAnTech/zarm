{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "paths": {
      "zarm": ["../packages/zarm/src/index.ts"],
      "zarm/es/*": ["../packages/zarm/src/*"],
      "zarm/lib/*": ["../packages/zarm/src/*"],
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts"]
}
